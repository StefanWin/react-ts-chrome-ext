{
  "name": "react-ts-chrome-ext",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
      "service_worker": "background.js"
  },
  "permissions": [
      "storage",
      "tabs"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "/icons/16x16.png",
          "32": "/icons/32x32.png"
      }
  },
  "icons": {
      "16": "/icons/16x16.png",
      "32": "/icons/32x32.png"
  },
  "options_page": "options.html"
}